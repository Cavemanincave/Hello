import tkinter
from tkinter import Label

#from tkinter import *

root = tkinter.Tk()

root.title("Hello world")
root.iconbitmap(default="free-icon-font-eye-3917104.ico")
root.geometry("300x250")

label_1_Hello_world = Label(text = "Hello world!!!")
label_1_Hello_world.pack()

root.mainloop()